{
    "manifest_version": 3,
    "name": "CUD++",
    "description": "Adds dark theme to cudplus",
    "version": "1.0.10",
    "content_scripts": [
        {
            "matches": ["https://cudplus.onsmart.school/*"],
            "js": ["src/replace_elements.js", "src/insert.js"],
            "css": ["assets/style.css"]
        },
        {
            "matches": ["https://cudplus.onsmart.school/home"],
            "js": ["src/store_default_image.js"]
        }
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "assets/cud_plus_logo_dark.svg",
                "assets/colors.json",
                "src/networkMonitor.js",
                "src/requestHandler.js",
                "src/makeHomeworkTable.js"
            ],
            "matches": ["https://cudplus.onsmart.school/*"]
        }
    ],
    "host_permissions": ["https://cudplus.onsmart.school/*"],
    "permissions": ["storage"],
    "icons": { "128": "assets/cud++.png" }
}
