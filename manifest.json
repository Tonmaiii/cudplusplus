{
    "manifest_version": 3,
    "name": "CUD++",
    "description": "Adds dark theme to cudplus",
    "version": "1.0.4",
    "content_scripts": [
        {
            "matches": ["https://cudplus.onsmart.school/*"],
            "js": ["replace_elements.js"],
            "css": ["style.css"]
        }
    ],
    "web_accessible_resources": [
        {
            "resources": ["cud_plus_logo_dark.svg"],
            "matches": ["https://cudplus.onsmart.school/*"]
        }
    ],
    "icons": { "128": "cud++.png" }
}
